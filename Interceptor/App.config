<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="file" xsi:type="File"
          layout="${longdate} ${logger} ${message}"
          fileName="${basedir}/${shortdate}.log" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="file" />
    </rules>
  </nlog>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DefaultJsonFileLocation" value="D:\src\GitHub\Interceptor\Interceptor\Json\"/>
    <add key="ConfigFileLocation" value="D:\src\GitHub\Interceptor\Interceptor\config.xml"/>
    <add key="LogRequests" value="true"/>
    <add key="LogRequestsFilter" value="*/api/*"/>
  </appSettings>
</configuration>